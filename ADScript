Import-Module ActiveDirectory

$UserList = Import-Csv -Path 'C:\Users\gordn\Downloads\new.csv'

foreach($User in $UserList) {
    $NewUser = @{
        Name = $User.first + " " + $User.last
        GivenName = $User.first
        SurName = $User.last
        SamAccountName = $User.first + " " + $User.last
        Department = $User.department
        Title = $User.title
        UserPrincipalName = $new.GivenName.Substring(0,1) + $new.Surname + "@globex.com"
        Path = "OU=Employees, DC=yourdomain, DC=com"
        Enabled = $true
    }
    Get-ADUser $NewUser
}